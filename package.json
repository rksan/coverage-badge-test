{
  "scripts": {
    "rm-dir": "rm -rf coverage | rm -rf .nyc_output",
    "mk-dir": "mkdir coverage",
    "make": "node ./node_modules/make/bin/make",
    "jscover": "node js-cover",
    "test:mocha": "YOURPACKAGE_COVERAGE=1 node ./node_modules/mocha/bin/mocha test -R mocha-lcov-reporter",
    "test": "nyc mocha -R spec",
    "precoverage-report": "npm run rm-dir",
    "coverage-report": "npm run test && nyc report --reporter=text-lcov",
    "coverage-github-action": "npm run coverage-report | coveralls"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "coveralls": "^3.1.1",
